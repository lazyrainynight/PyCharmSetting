<keymap version="1" name="SKeymap" parent="$default">
  <action id="Annotate">
    <keyboard-shortcut first-keystroke="shift ctrl back_slash" />
  </action>
  <action id="CloseAllEditorsButActive">
    <keyboard-shortcut first-keystroke="shift ctrl alt f4" />
  </action>
  <action id="CloseAllUnpinnedEditors">
    <keyboard-shortcut first-keystroke="shift ctrl alt z" />
  </action>
  <action id="Compare.SameVersion">
    <keyboard-shortcut first-keystroke="shift ctrl alt quote" />
  </action>
  <action id="Compare.Selected">
    <keyboard-shortcut first-keystroke="shift alt quote" />
  </action>
  <action id="Diff.ShowInExternalTool">
    <keyboard-shortcut first-keystroke="shift ctrl alt semicolon" />
  </action>
  <action id="EditorResetFontSize">
    <mouse-shortcut keystroke="control button2" />
  </action>
  <action id="GotoNextError">
    <keyboard-shortcut first-keystroke="f12" />
  </action>
  <action id="JumpToLastWindow" />
  <action id="Macro.Reformat -&gt; Save">
    <keyboard-shortcut first-keystroke="shift ctrl s" />
  </action>
  <action id="PinActiveTab">
    <keyboard-shortcut first-keystroke="shift alt z" />
  </action>
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="f2" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="shift ctrl alt close_bracket" />
  </action>
  <action id="ToggleInlineHintsAction">
    <keyboard-shortcut first-keystroke="shift alt equals" />
  </action>
  <action id="Vcs.ShowTabbedFileHistory">
    <keyboard-shortcut first-keystroke="shift ctrl alt back_slash" />
  </action>
</keymap>